    <record id="jadwal_dosen_rule" model="ir.rule">
        <field name="name">Jadwal Dosen</field>
        <field name="model_id" ref="model_master_jadwal"/>
        <field name="domain_force">['|','|',('employee_id.user_id','=',user.id),('fakultas_id','=',user.fakultas_id.id),('user_id.is_baak','=',user.is_baak)]</field>
        <field name="groups" eval="[(4, ref('group_univ_external_baak'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>        
    </record>

