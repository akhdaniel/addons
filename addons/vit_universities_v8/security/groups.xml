        <field name="name">Dosen Wali</field>
        <field name="model_id" ref="model_master_kelas"/>
        <field name="domain_force">['|','|','|',('employee_id.user_id','=',user.id),('fakultas_id','=',user.fakultas_id.id),('prodi_id','=',user.partner_id.prodi_id.id),('user_id.is_baak','=',user.is_baak)]</field>
