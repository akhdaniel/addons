        <!-- Academic Years Information From View -->
        <record model="ir.ui.view" id="view_academic_year_form">
            <field name="name">academic.year.form</field>
            <field name="model">academic.year</field>
            <field name="arch" type="xml">
                <form string="Academic Years" version="7.0">
                    <header>
                        <button name="create_period" string="Create Months" type="object"/>
                    </header>
                    <sheet>
                        <group col="4" colspan="4">
                            <group>
                                <field name="code" placeholder="Code"/> 
                                <field name="name" placeholder="Name"/> 
                                <label for="date_start" string="Tanggal"/>
                                <div>
                                    <field name="date_start" nolabel="1" class="oe_inline" placeholder="From"/><label string="-" class="oe_inline"/>
                                    <field name="date_stop" nolabel="1" class="oe_inline" placeholder="To"/>
                                </div>

                            </group>
                            <group> 
                                <field name="mekanisme_nilai"/>                             
                                <field name="max_mk"/>
                            </group>
                        </group>
                        <notebook colspan="4">
                            <page string="Months">
                                <field name="month_ids" nolabel="1" colspan="4" readonly="1">
                                    <tree string="Months">
                                        <field name="name"/>  
                                        <field name="code"/>
                                        <field name="date_start"/>
                                        <field name="date_stop"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                        <newline/>
                        <separator string="Description" colspan="4"/>
                        <field name="description" colspan="4"/>
                   </sheet>
                </form>
            </field>
        </record>

