                            </group>

                            <group >
                                <field name="gelar_id"/>
                                <field name="semester_id" widget="selection"/>
                                <field name="jenjang"/> 
                            </group>                          
                        </group>
                    </sheet>                     
                </form>               
            </field>
        </record>

