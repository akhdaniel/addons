            <field name="model">master.prodi</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Program Studi" version="7.0">
                    <sheet>
                        <group>
                            <group>
                                <field name="kode"/>
                                <field name="kode_dikti"/>
                                <field name="kode_internal"/>
                                <field name="name"/>                    
