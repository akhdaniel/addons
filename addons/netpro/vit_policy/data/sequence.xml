<openerp>
	<data>
		<!-- POLICY NO -->
		<record model="ir.sequence.type" id="policy_seq_code">
			<field name="name">Policy Sequence</field>
			<field name="code">policy_seq</field>
		</record>

		<record model="ir.sequence" id="policy_seq">
			<field name="name">Policy Sequence</field>
			<field name="code">policy_seq</field>
			<field name="padding">5</field>
			<field name="number_next_actual">5</field>
			<field name="number_increment">1</field>
			<field name="implementation">standard</field>
			<field name="prefix">%(year)s/%(month)s/</field>
		</record>

		<!-- PNO ( SEQUENCE NOMOR UNTUK DOCUMENT SECARA SISTEM ) -->
		<record model="ir.sequence.type" id="pno_seq_code">
			<field name="name">PNO Sequence</field>
			<field name="code">pno_seq</field>
		</record>

		<record model="ir.sequence" id="pno_seq">
			<field name="name">PNO Sequence</field>
			<field name="code">pno_seq</field>
			<field name="padding">5</field>
			<field name="number_next_actual">5</field>
			<field name="number_increment">1</field>
			<field name="implementation">standard</field>
			<field name="prefix">%(year)s/%(month)s/</field>
		</record>
	</data>
</openerp>