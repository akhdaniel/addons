  <story>
    <para style="Standard">[[ repeatIn(objects,'invoice') ]]</para>
    <para style="terp_default_8">[[ setLang(invoice.partner_id.lang) ]]</para>
    <blockTable colWidths="199.0,177.0,74.0,131.0" style="Table1">
      <tr>
        <td>
          <para style="P3">[[invoice .user_id.company_id.name]]</para>
          <para style="P4">[[invoice.user_id.company_id.street]]</para>
          <para style="P4">[[invoice.user_id.company_id.street2]]</para>
          <para style="P4">Tlp. [[invoice.user_id.company_id.phone]] Fax. [[invoice.user_id.company_id.fax]]</para>
          <para style="P4">NPWP . [[invoice .user_id.company_id.vat]]</para>
          <para style="P4">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P15">FAKTUR PENJUALAN</para>
        </td>
        <td>
          <para style="P5">No Faktur</para>
          <para style="P5">Ref</para>
          <para style="P4">Tanggal</para>
          <para style="P4">Jatuh Tempo</para>
          <para style="P4">TOP</para>
        </td>
        <td>
          <para style="P4">[[ invoice.number or '-']]</para>
          <para style="P4">[[invoice.origin or '-']]</para>
          <para style="P4">[[ formatLang(invoice.date_invoice,date = True) or '-']]</para>
          <para style="P4">[[ formatLang(invoice.date_due,date = True) or '-']]</para>
          <para style="P4">[[ (invoice.payment_term and invoice.payment_term.name) or '-' ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="84.0,160.0,132.0,74.0,131.0" style="Table9">
      <tr>
        <td>
          <para style="P4">Kode Pelanggan</para>
          <para style="P4">Nama Pelanggan</para>
        </td>
        <td>
          <para style="P4">[[(invoice.partner_code)]]</para>
          <para style="P4">[[ (invoice.partner_id and invoice.partner_id.title and invoice.partner_id.title.name) or '' ]] [[ (invoice.partner_id and invoice.partner_id.name) or '' ]]</para>
          <para style="P4">[[ invoice.partner_id and display_address(invoice.partner_id) ]] </para>
          <para style="P4">Tel. : [[ (invoice.partner_id.phone) or removeParentNode('para') ]]</para>
          <para style="P4">Fax : [[ (invoice.partner_id.fax) or removeParentNode('para') ]]</para>
          <para style="P4">NPWP : [[ (invoice.partner_id.name_tax) or removeParentNode('para') ]]</para>
        </td>
        <td>
          <para style="P15">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P4">Gudang</para>
          <para style="P4">Kode Salesman</para>
          <para style="P4">Salesman</para>
        </td>
        <td>
          <para style="P4">[[ invoice.location_id.name ]]</para>
          <para style="P4">[[ (invoice.nik) or '' ]]</para>
          <para style="P4">[[ (invoice.user_id and invoice.user_id.name) or '' ]]</para>
          <para style="P4">
            <font color="white"> </font>
          </para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="189.0,84.0,91.0,99.0,118.0" repeatRows="1" style="Table4">
      <tr>
        <td>
          <para style="P9">Kode dan Nama Barang</para>
        </td>
        <td>
          <para style="P11">Qty Besar</para>
        </td>
        <td>
          <para style="P11">Qty Kecil</para>
        </td>
        <td>
          <para style="P11">Harga</para>
        </td>
        <td>
          <para style="P10">Total</para>
        </td>
      </tr>
    </blockTable>
    <section>
      <para style="terp_default_1">[[repeatIn(invoice.invoice_line,'line')]]</para>
      <blockTable colWidths="189.0,84.0,90.0,99.0,117.0" style="Table5">
        <tr>
          <td>
            <para style="P1">[[ format(line.name) ]] </para>
          </td>
          <td>
            <para style="P8">[[ formatLang(line.qty) or '-']] [[ line.uos_id.name]]</para>
          </td>
          <td>
            <para style="P7">[[ formatLang(line.quantity2) or '-']] [[line.uom_id.name ]]</para>
          </td>
          <td>
            <para style="P8">[[ formatLang(line.price_unit , digits=get_digits(dp='Product Price'))]]</para>
          </td>
          <td>
            <para style="P8">[[ formatLang(line.price_subtotal)]] </para>
          </td>
        </tr>
      </blockTable>
    </section>
    <blockTable colWidths="391.0,62.0,10.0,117.0" style="Table6">
      <tr>
        <td>
          <para style="P2">Ket:</para>
          <para style="P1">*Penagihan Hanya dilakukan dengan faktur asli</para>
          <para style="P1">*Pembayaran dengan BG dianggap lunas jika BG cair</para>
          <para style="P1">*Komplain kekurangan tidak dilayani jika faktur telah di tanda tangani</para>
          <para style="P1">*Faktur pajak diterbitkan sesuai tanggal terima barang</para>
          <para style="P1">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P2">Faktur </para>
          <para style="P2">Ppn</para>
          <para style="P18">Faktur+PPn </para>
        </td>
        <td>
          <para style="P2">:</para>
          <para style="P2">:</para>
          <para style="P2">:</para>
        </td>
        <td>
          <para style="P8">[[ formatLang(invoice.amount_untaxed) ]]</para>
          <para style="P8">[[ formatLang(invoice.amount_tax) ]]</para>
          <para style="P23">[[ formatLang(invoice.amount_total)]]</para>
        </td>
      </tr>
    </blockTable>
