      <tr>
        <td>
          <para style="P14">Hormat Kami,</para>
          <para style="P14">
            <font color="white"> </font>
          </para>
          <para style="P14">
            <font color="white"> </font>
          </para>
          <para style="P25">[[ (invoice.user_id and invoice.user_id.name) or '' ]]</para>
        </td>
        <td>
          <para style="P14">Disetujui Oleh,</para>
        </td>
        <td>
          <para style="P14">Pengirim,</para>
        </td>
        <td>
          <para style="P14">Diterima Oleh,</para>
        </td>
      </tr>
