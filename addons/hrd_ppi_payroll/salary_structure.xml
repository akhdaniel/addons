        <record id="hr_payroll_rules_overtime" model="hr.salary.rule">
	        <field name="name">OVERTIME</field>
	        <field name="code">OVERTIME</field>
	        <field name="category_id" ref="hr_payroll.GROSS"/>
	        <field name="condition_select">none</field>
	        <field name="amount_select">code</field>
			<field name="sequence">2</field>
	        <field name="amount_python_compute">result = ((((contract.wage + payslip.umak + payslip.ut + payslip.uml)*75)/100) /173) * worked_days.OVERTIME.number_of_hours </field>
	    </record>
	    <record id="hr_payroll_rules_overtime_meal" model="hr.salary.rule">
