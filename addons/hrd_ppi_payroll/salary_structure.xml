	        <field name="condition_select">none</field>
	        <field name="amount_select">code</field>
			<field name="sequence">3</field>
	        <field name="amount_python_compute">result = worked_days.UANG_MAKAN_LEMBUR.number_of_days* contract.makan </field>
	    </record>
