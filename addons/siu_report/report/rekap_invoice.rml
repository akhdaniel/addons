  <section>  
  
  <para>[[ repeatIn(o.partner_ids, 'x') ]]</para>
  <blockTable colWidths="550.0" style="Table001">
      <tr>
        <td><para style="terp_default_Left_Bold_8">[[ x.name ]]</para></td>
      </tr>
  </blockTable>
	  <section>
	  <para>[[ repeatIn(get_invoice(x.id, o)[0], 'i') ]]</para>
	  <blockTable colWidths="80.0,80.0,250.0,70.0,70.0" style="Table001">
	      <tr>
	        <td><para style="terp_default_8">[[ i['number'] ]]</para></td>
	        <td><para style="terp_default_8">[[ i['date'] ]]</para></td>
	        <td><para style="terp_default_8">[[ i['ucapan'] ]]</para></td>
	        <td><para style="terp_default_Right_8">[[ i['harga'] ]]</para></td>
	        <td><para style="terp_default_Center_Bold_8">-</para></td>
	      </tr>
	  </blockTable>
	  </section>
