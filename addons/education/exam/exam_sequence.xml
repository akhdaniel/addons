<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for exam -->
        <record id="seq_exam_exam_type" model="ir.sequence.type">
            <field name="name">Examintaion Code</field>
            <field name="code">exam.exam</field>
        </record>

        <record id="seq_exam_exam" model="ir.sequence">
            <field name="name">Exam</field>
            <field name="code">exam.exam</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>
        
        <!-- Sequences for additional exam -->
        <record id="seq_additional_exam_type" model="ir.sequence.type">
            <field name="name">Examintaion Code</field>
            <field name="code">additional.exam</field>
        </record>

        <record id="seq_additional_exam" model="ir.sequence">
            <field name="name">Additional Exam</field>
            <field name="code">additional.exam</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>

    </data>
</openerp>
