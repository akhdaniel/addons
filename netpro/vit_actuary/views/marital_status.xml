<openerp>
<data>
    <record model="ir.ui.view" id="view_netpro_marital_status_form">
        <field name="name">netpro.marital_status.form</field>
        <field name="model">netpro.marital_status</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="netpro.marital_status">
                <group>
                    <field name="name" select="1"/>
                    <field name="short_desc" select="1"/>
                    <field name="description" colspan="4" select="2"/>
                </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_netpro_marital_status_tree">
        <field name="name">netpro.marital_status.tree</field>
        <field name="model">netpro.marital_status</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="netpro.marital_status">
                <field name="name"/>
                <field name="short_desc"/>
                <field name="description"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_netpro_marital_status">
        <field name="name">Marital Status</field>
        <field name="res_model">netpro.marital_status</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
</data>
</openerp>
